<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".AdminSideofThings.ECEditCandidate">

    <EditText
        android:id="@+id/ECEditName"
        android:layout_width="375dp"
        android:layout_height="50dp"
        android:layout_marginStart="18dp"
        android:layout_marginTop="24dp"
        android:hint="name"
        android:textSize="24sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/ECEditPosition"
        android:layout_width="375dp"
        android:layout_height="50dp"
        android:layout_marginStart="18dp"
        android:layout_marginTop="12dp"
        android:hint="position"
        android:textSize="24sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/ECEditName" />

    <EditText
        android:id="@+id/ECEditAge"
        android:layout_width="375dp"
        android:layout_height="50dp"
        android:layout_marginStart="18dp"
        android:layout_marginTop="12dp"
        android:hint="age"
        android:textSize="24sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/ECEditPosition" />

    <EditText
        android:id="@+id/ECEditAddress"
        android:layout_width="375dp"
        android:layout_height="50dp"
        android:layout_marginStart="18dp"
        android:layout_marginTop="12dp"
        android:hint="address"
        android:textSize="24sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/ECEditAge" />

    <EditText
        android:id="@+id/ECEditCivil"
        android:layout_width="375dp"
        android:layout_height="50dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="12dp"
        android:hint="civil"
        android:textSize="24sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/ECEditAddress" />

    <EditText
        android:id="@+id/ECEditBirth"
        android:layout_width="375dp"
        android:layout_height="50dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="12dp"
        android:hint="birth"
        android:textSize="24sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/ECEditCivil" />

    <EditText
        android:id="@+id/ECEditEmail"
        android:layout_width="375dp"
        android:layout_height="50dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="12dp"
        android:hint="email"
        android:textSize="24sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/ECEditBirth" />

    <EditText
        android:id="@+id/ECEditGender"
        android:layout_width="375dp"
        android:layout_height="50dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="12dp"
        android:hint="gender"
        android:textSize="24sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/ECEditEmail" />

    <EditText
        android:id="@+id/ECEditID"
        android:layout_width="375dp"
        android:layout_height="50dp"
        android:layout_marginStart="18dp"
        android:layout_marginTop="12dp"
        android:hint="membership id"
        android:textSize="24sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/ECEditGender" />

    <EditText
        android:id="@+id/ECEditVision"
        android:layout_width="375dp"
        android:layout_height="50dp"
        android:layout_marginStart="18dp"
        android:layout_marginTop="12dp"
        android:hint="vision"
        android:textSize="24sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/ECEditID" />

    <Button
        android:id="@+id/SaveEdit"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="12dp"
        android:text="Save"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/ECEditVision" />

</androidx.constraintlayout.widget.ConstraintLayout>